        },
        "vi" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "LingLook chưa được kích hoạt. Vui lòng bật LingLook trong tuỳ chọn Phần mở rộng của Safari."
          }
        },
        "zh-Hans" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "LingLook 已关闭。请在 Safari 扩展偏好设置中启用 LingLook。"
          }
        },
        "zh-Hant" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "LingLook 已關閉。請在 Safari 延伸功能偏好設定中啟用 LingLook。"
